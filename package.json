{
  "name": "deepseek-vscode",
  "version": "0.0.1",
  "description": "VS Code extension for DeepSeek AI integration.",
  "author": "Ibrahim Ragab",
  "private": true,
  "license": "MIT",
  "engines": {
    "vscode": "^1.85.0" 
  },
  "scripts": {
    "build": "yarn run compile",
    "compile": "tsc -p ./",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "yarn run build && code .",
    "start:dev": "yarn run build && code --extensionDevelopmentPath=.",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@vscode/vsce": "^3.2.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "@types/vscode": "^1.81.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "configuration": {
      "title": "DeepSeek Coder",
      "properties": {
        "deepseekCoder.enableLogging": {
          "type": "boolean",
          "default": true,
          "description": "Enable or disable logging for AI interactions."
        }
      }
    },
    "commands":[
      {
        "command": "deepseek.runModel",
        "title": "Run DeepSeek Model"
      }
    ]
  }
}
